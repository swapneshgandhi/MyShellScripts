#!/bin/bash -l


for ((i=1,j=1;j<=1000;i=j,j=k));do
	k=$((i+j))

echo "running benchmarking for $k users"

ab -t 300 -n 150000000 -c $k -H "Referer: http://player.ooyala.com/static/cacheable/1f0e08deb6420baa72523902ac8a9691/player_v2.swf?player=dcb79e2098c94889a1b9f2af6280b45d/[[DYNAMIC]]/2" -H "Content-type: application/x-www-form-urlencoded" -H "X-Verify: &sig=L07GhbpCRcxxEtCy6mkp" -T "application/x-www-form-urlencoded" http://logger-next-staging3.us-east-1.ooyala.com/verify?ts=1438977514272 | tee $(pwd)/report_${k}_users.txt

done

